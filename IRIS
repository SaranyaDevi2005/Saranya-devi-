# Commented out IPython magic to ensure Python compatibility.
import numpy as np
import matplotlib.pyplot as plt
import seaborn as sns
import pandas as pd
# %matplotlib inline

columns=['sepal_length','sepal_width','petal_length','petal_width','species']  # Removed space in 'petal_width'
df=pd.read_csv('/content/IRIS.csv',names=columns)
df.head()

df.describe()

sns.catplot(data=df, x='species', y='sepal_length', hue='species', kind='boxen')

sns.pairplot(df,hue='Class_labels')

data=df.values
X=data[:,0:4]
Y=data[:,4]
print(Y)

from sklearn.model_selection import train_test_split
X_train, X_test, Y_train, Y_test = train_test_split(X,Y, test_size=0.2)
print(X_test)
print(X_train)
